if player.Character then
player.Character.HumanoidRootPart.CFrame = CFrame.new(-519, 71.5628433, -879.516663, 0.0660492629, -1.43967524e-21, 0.997816384, 3.67725403e-22, 1, 1.4184847e-21, -0.997816384, 2.73232564e-22, 0.0660492629)
end
end
end)

