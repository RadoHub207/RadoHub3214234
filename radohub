aimbot.Name = "aimbot"
aimbot.Parent = assassin
aimbot.BackgroundColor3 = Color3.new(0.243137, 0.243137, 1)
aimbot.BackgroundTransparency = 0.30000001192093
aimbot.BorderColor3 = Color3.new(1, 1, 1)
aimbot.BorderSizePixel = 0
aimbot.Position = UDim2.new(0.0956521705, 0, 0.525006592, 0)
aimbot.Size = UDim2.new(0, 91, 0, 50)
aimbot.Font = Enum.Font.Highway
aimbot.FontSize = Enum.FontSize.Size32
aimbot.Text = "Aimbot"
aimbot.TextColor3 = Color3.new(1, 1, 1)
aimbot.TextScaled = true
aimbot.TextSize = 30
aimbot.TextWrapped = true
aimbot.MouseButton1Down:connect(function()
    wait()
local Player = game:GetService("Players").LocalPlayer
local Mouse = Player:GetMouse()
local findNearestTorso = function(pos)
local list = workspace:GetChildren()
local torso = nil
local dist = 999
local temp = nil
local human = nil
local temp2 = nil
for x = 1, #list do
temp2 = list[x]
if (temp2.className == "Model") and (temp2 ~= Player.Character) then
temp = temp2:findFirstChild("HumanoidRootPart")
human = temp2:findFirstChild("Humanoid")
if (temp ~= nil) and (human ~= nil) and (human.Health > 0) then
if (temp.Position - pos).magnitude < dist then
torso = temp
dist = (temp.Position - pos).magnitude
end
end
end
end
return torso
end

local Start = function()
if workspace:findFirstChild("KnifeHost") then
Connection = workspace.KnifeHost.ChildAdded:connect(function(Knife)
Connection:disconnect()
if Knife then
if Player and Player.Character and Player.Character:findFirstChild("Head") then
if findNearestTorso(Player.Character.Head.Position) then
local Torso = findNearestTorso(Player.Character.Head.Position)
Knife.CFrame = Torso.CFrame
for i=1, 3 do
wait()
if Knife and Torso then
Knife.CFrame = Torso.CFrame
end
end
end
end
end
end)
else
print("No host")
end
end
Mouse.Button1Up:connect(Start)
end)


